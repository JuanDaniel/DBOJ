<ul class="actions">
    {% if extras is defined %}
        {% for extra in extras %}
            <li><a href="{{ extra.path }}" title="{{ extra.title }}"><i class="{{ extra.icon }}"></i></a></li>
        {% endfor %}
    {% endif %}
    {% if path_edit is defined and path_delete is defined %}<li><a href="{{ path(path_edit, { 'id': entity.id }) }}" title="{{ title_edit }}"><i class="fa fa-edit"></i></a></li>
     <li class="last"><a href="{{ path(path_delete, { 'id': entity.id }) }}" title="{{ title_delete }}" onclick="return confirm('{{ msg_confirm }}')"><i class="fa fa-trash-o"></i></a></li>{% endif %}
</ul>